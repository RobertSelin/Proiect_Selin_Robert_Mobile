            Title="Plans"
            ContentTemplate="{DataTemplate local:AboutPage}"/>-->
        <ShellContent
            Title="My Plan List"
            ContentTemplate="{DataTemplate local:ListEntryPage}"/>
        <ShellContent
            Title="My Courts"
            ContentTemplate="{DataTemplate local:CourtEntryPage}"/>
    </TabBar>

</Shell>
